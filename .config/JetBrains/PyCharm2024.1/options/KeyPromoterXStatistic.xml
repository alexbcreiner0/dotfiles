<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Collapse All">
        <Item>
          <IdeaActionID>CollapseAll</IdeaActionID>
          <ShortCut>'Ctrl+NumPad -' or 'Ctrl+Minus'</ShortCut>
          <Description>Collapse All</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open...">
        <Item>
          <IdeaActionID>OpenFile</IdeaActionID>
          <ShortCut>'Ctrl+O'</ShortCut>
          <Description>Open...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>4</Count>
          <Hit>4</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Reformat Code">
        <Item>
          <IdeaActionID>ReformatCode</IdeaActionID>
          <ShortCut>'Ctrl+Alt+L'</ShortCut>
          <Description>Reformat Code</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run</Description>
          <Count>2</Count>
          <Hit>2</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Select Cell Above">
        <Item>
          <IdeaActionID>NotebookSelectCellAboveAction</IdeaActionID>
          <ShortCut>'Ctrl+Up'</ShortCut>
          <Description>Select Cell Above</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>